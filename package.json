    "karma": "^0.13.10",
    "karma-browserify": "^4.3.0",
    "karma-coverage": "^0.5.2",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "phantomjs": "^1.9.18",
    "uglify-js": "^2.4.24",
    "watchify": "^3.4.0"
